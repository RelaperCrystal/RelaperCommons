        <member name="T:RelaperCommons.BlipExtensions">
            <summary>Extension methods of <see cref="T:Rage.Blip" /> class.</summary>
            <seealso cref="T:Rage.Blip" />
        </member>
        <member name="M:RelaperCommons.BlipExtensions.SetColor(Rage.Blip,RelaperCommons.Entities.BlipColor)">
            <summary>Sets the color of this <see cref="T:Rage.Blip" /> to the predefined system blip color.</summary>
            <param name="blip">The blip to set color.</param>
            <param name="color">The color.</param>
            <remarks>
              <note type="important">Do not set <see cref="P:Rage.Blip.Color" /> once you set this color. If you want to set anyway, imagine it's secondary color (mix with this).</note>
            </remarks>
        </member>
        <member name="M:RelaperCommons.BlipExtensions.SetRouteColor(Rage.Blip,RelaperCommons.Entities.BlipColor)">
            <summary>Sets the route color of the <see cref="T:Rage.Blip" /> to a predefined system color.</summary>
            <param name="blip">The blip.</param>
            <param name="color">The color.</param>
        </member>
        <member name="M:RelaperCommons.BlipExtensions.EnableRoute(Rage.Blip,RelaperCommons.Entities.BlipColor)">
            <inheritdoc cref="M:Rage.Blip.EnableRoute(System.Drawing.Color)"/>
            <param name="blip">The blip to set the color.</param>
            <param name="color"><inheritdoc /></param>
        </member>
        <member name="T:RelaperCommons.Entities.BlipColor">
