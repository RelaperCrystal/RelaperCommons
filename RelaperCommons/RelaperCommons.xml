        <member name="T:RelaperCommons.FirstResponse.RadioUtil">
            <summary>
            Provides tools to handle radio communication.
            </summary>
        </member>
        <member name="M:RelaperCommons.FirstResponse.RadioUtil.DisplayRadioQuote(System.String,System.String)">
            <summary>Displays an notification showing the speaker speaking the text.</summary>
            <param name="speaker">The speaker.</param>
            <param name="text">The text.</param>
            <returns>The handle of the notification.</returns>
            <remarks>
            <c>{player}</c> in the text will be replaced to the player's character name
            (e.g. Jenny Sandusky).
            </remarks>
        </member>
        <member name="M:RelaperCommons.FirstResponse.RadioUtil.DisplayRadioQuoteUsingPosition(System.String,System.String,Rage.Vector3,System.Boolean)">
            <summary>Displays the radio quote using position.</summary>
            <param name="speaker">The speaker.</param>
            <param name="text">The text.</param>
            <param name="position">The position.</param>
            <param name="street">if set to <c>true</c>, displays the street name.</param>
            <returns>
              The handle of the notification.
            </returns>
            <remarks>
            <c>{position}</c> in the text will be replaced to the position display name, and <c>{player}</c> in the text will be replaced to the player's character name
            (e.g. Jenny Sandusky).
            </remarks>
        </member>
        <member name="T:RelaperCommons.LocationUtil">
            <summary>
              <para>Provides methods to handle locations.</para>
            </summary>
        </member>
        <member name="M:RelaperCommons.LocationUtil.GetRandomPositionOnStreet(System.Single,System.Single)">
            <summary>Gets a position around the player (usually on motorized roads) which is relatively safer for <see cref="T:Rage.Vehicle"/>s to cruise.</summary>
            <param name="minimumDistance">The minimum distance.</param>
            <param name="maximumDistance">The maximum distance.</param>
            <returns>
              <para>A <see cref="T:Rage.Vector3" /> indicating the generated spawn point.</para>
            </returns>
        </member>
        <member name="M:RelaperCommons.LocationUtil.GetRandomPositionOnSidewalk(System.Single,System.Single)">
            <summary>Gets a position around the player (usually on sidewalk) which is relatively safer for <see cref="T:Rage.Ped" />s to walk.</summary>
            <param name="minimumDistance">The minimum distance.</param>
            <param name="maximumDistance">The maximum distance.</param>
            <returns>If success, the generated position; otherwise, returns <see cref="P:Rage.Vector3.Zero" />.<br /></returns>
        </member>
        <member name="T:RelaperCommons.VehicleExtensions">
            <summary>
            Provides extensions to <see cref="T:Rage.Vehicle"/>.
            </summary>
        </member>
        <member name="M:RelaperCommons.VehicleExtensions.GetDisplayName(Rage.Vehicle)">
            <summary>
            Gets the localized display name of the vehicle.
            </summary>
            <param name="vehicle">The vehicle.</param>
            <returns>The display name of vehicle, or <see langword="null"/> if not found.</returns>
        </member>
        <member name="M:RelaperCommons.VehicleExtensions.TryGetDisplayNameOfVehicleModel(Rage.Vehicle,System.String@)">
            <summary>Tries to get the display name of specified vehicle model.</summary>
            <param name="vehicle">The vehicle.</param>
            <param name="result">The result.</param>
            <returns>
              <para>Returns true if a proper name has been found; otherwise, false.</para>
            </returns>
        </member>
